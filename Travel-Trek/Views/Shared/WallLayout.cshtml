<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Wall</title>
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
    <link rel="icon" href="~/Content/images/icon.jpg" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.Partial("Partials/_wallNavBar")
   <!-- Html.Partial("_accountsModal")-->
    
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer >
            <p>&copy; @DateTime.Now.Year - Travel Trek</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/lib")
    <script src="~/Scripts/Login-Register/modals.js"></script>
    <!-- MDB -->
    <script
        type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.5.0/mdb.min.js"
    ></script>
    @RenderSection("scripts", required: false)

</body>
</html>